{
  "name": "webpack_boilerplate",
  "version": "1.0.0",
  "description": "webpack_boilerplate",
  "main": "app.js",
  "scripts": {
    "start": "nodemon ./bin/www",
    "build": "webpack --watch --config ./configs/webpack.config.js",
